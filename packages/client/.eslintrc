{
  "extends": ["airbnb", "../../.eslintrc"],
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "browser": true
  },
  "globals": {
    "React": true
  },
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/prop-types": 0,
    "jsx-a11y/img-has-alt": 0,
    "react/jsx-no-bind": 0,
    "react/function-component-definition": 0
  }
}
